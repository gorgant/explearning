{
  "name": "expn-public",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-app:sandbox": "ng build -c=sandbox",
    "build:client-app:prod": "ng build --configuration production",
    "build:server-app:sandbox": "ng run expn-public:server:sandbox",
    "build:server-app:prod": "ng run expn-public:server:production",
    "build-all:sandbox": "npm-run-all build:client-app:sandbox build:server-app:sandbox",
    "build-all:prod": "npm-run-all build:client-app:prod build:server-app:prod",
    "move-index": "mv ./dist/expn-public/browser/index.html ./functions/lib/app-bundle/index-server.html",
    "replace-robots-file:sandbox": "cp ./src/environments/sandbox-robot/robots.txt ./dist/expn-public/browser/robots.txt",
    "deploy": "firebase deploy",
    "build-and-deploy:sandbox": "npm-run-all build-all:sandbox move-index replace-robots-file:sandbox deploy",
    "build-and-deploy:prod": "npm-run-all build-all:prod move-index deploy",
    "dev:ssr": "ng run expn-public:serve-ssr",
    "serve:ssr": "node dist/expn-public/server/main.js",
    "build:ssr": "ng build --configuration production && ng run expn-public:server:production",
    "prerender": "ng run expn-public:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.11",
    "@angular/cdk": "^12.2.11",
    "@angular/common": "~12.2.11",
    "@angular/compiler": "~12.2.11",
    "@angular/core": "~12.2.11",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~12.2.11",
    "@angular/material": "^12.2.11",
    "@angular/material-moment-adapter": "^12.2.11",
    "@angular/platform-browser": "~12.2.11",
    "@angular/platform-browser-dynamic": "~12.2.11",
    "@angular/platform-server": "~12.2.11",
    "@angular/router": "~12.2.11",
    "@google-cloud/firestore": "^4.8.1",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/entity": "^10.1.2",
    "@ngrx/router-store": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "@nguniversal/express-engine": "^11.0.1",
    "compression": "^1.7.4",
    "core-js": "^3.8.2",
    "express": "^4.17.1",
    "firebase": "^8.2.3",
    "moment": "^2.29.1",
    "ng-lazyload-image": "^9.1.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1202.11",
    "@angular-devkit/build-angular": "~12.2.11",
    "@angular/cli": "~12.2.11",
    "@angular/compiler-cli": "~12.2.11",
    "@angular/language-service": "~12.2.11",
    "@nguniversal/builders": "^11.0.1",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.21",
    "@types/stripe": "^8.0.416",
    "@types/stripe-v3": "^3.1.23",
    "@types/xml2js": "^0.4.7",
    "codelyzer": "^6.0.1",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.0",
    "firebase-functions-test": "^0.2.3",
    "firebase-tools": "^9.2.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.6",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "npm-run-all": "^4.1.5",
    "open": "^7.3.1",
    "protractor": "~7.0.0",
    "stripe": "^8.131.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "^4.3.5"
  }
}